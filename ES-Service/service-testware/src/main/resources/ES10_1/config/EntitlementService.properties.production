# ES configuration file

################################################################################
### RUC DB POOL TUNING
################################################################################

## Numbers of rows to prefetch when opening a result set
es.db.prefetch=250


################################################################################
### CHECK DATE RANGE, COUNTRY CODE MAPPING
################################################################################

## Provide settings for the ES check date range
es.checkDate.min=2000
es.checkDate.max=1

## Global Country Code Mapping
es.common.CountryCodeMapping.UK=GB

# Definition of product lines in SWOP replies that are considered consumer product lines
es.warrantyservice.swop.consumerProductLines=R6,MK

# ASTRO2 Access Filter: 
# A call to ASTRO2 is needed if the WarrantyInfo.SupportLine is equal to "R6" or "MK" or "MN" or "R4" or "72" or is empty.
es.WarrantyInfo.SupportLine.callAstro2=R6,MK,MN,R4,72


################################################################################
### IB SEARCH
################################################################################
es.ibsearch.swop.region=AM
es.ibsearch.swop.timeout=25000
es.ibsearch.swop.timeout.enable=false

################################################################################
### Routing Details
################################################################################gg
es.routingdetails.orchestration.region=AM

################################################################################
### SAP ACCESS CONFIGURATION
################################################################################

### --------------------------------------------------------
### Orchestration
### --------------------------------------------------------
# swop timeout
es.warrantyservice.orchestration.thread.timeout=15000
es.warrantyservice.orchestration.swop.enable=true

# contract timeout
es.contractservice.orchestration.thread.timeout=15000

## list of contract system to call 
## A coma separated list of portfolio to filter (G, S or C) 
es.contractservice.orchestration.systems=ODS, CQS
es.contractservice.ODS.filtering=G

# contract summary
es.contractsummaryservice.orchestration.thread.timeout.enable=false
es.contractsummaryservice.orchestration.thread.timeout=15000
es.contractsummaryservice.orchestration.systems=ODS, CQS


# which 'regions' to access in parallel - see below for per-region configuration
# This is used at integration time
es.sap.orchestration.regions=AM,EMEA,AP
es.sap.orchestration.regions.localregion=AM

## failover duration
es.sap.orchestration.failover.mode.duration=600000

## used implementation
es.sap.orchestration.poolImplementation=SAP
#es.sap.orchestration.regions.accessmethod=JCO
#es.sap.orchestration.regions.accessmethod=SOAP_CASTOR
es.sap.orchestration.regions.accessmethod=SOAP_METRO
#es.sap.orchestration.regions.soap.implementation=com.hp.es.service.orchestration.sap.SocketSapSOAPAccess
es.sap.orchestration.regions.soap.implementation=com.hp.es.service.orchestration.sap.MetroSapSOAPAccess

## SAP Integration thread control
es.sap.integration.thread.timeout=15000
es.sap.integration.thread.timeout.enable=false


