################################################################################
### BATCH ENTITLEMENT
################################################################################
# enable or disable the batch entitlement
batchEntitlement.enabled=false

# how many threads run in parallel (default=2)
batchEntitlement.threadCount=3

# how long a thread will wait when nothing is to do
# before it wakes up again
# milliseconds
batchEntitlement.threadIdleTime=60000

# If there are less requests running than specified in
# this parameter, it will start additional batch requests.
# If more requests are running in parallel, the batch
# processing is stopped.
# IF YOU INCREASE THE VALUE FOR maxParallelRequests, YOU ALSO HAVE TO INCREASE
# THE VALUE FOR batchEntitlement.maxconns IN THE db.properties FILE.
batchEntitlement.maxParallelRequests=3

# Specify the number of days a request should be kept in
# history.
# days
batchEntitlement.removeRequestsOlderThan=180

# Specify the maximum number of request in a CSV file.
# If the number of actual entries is larger than configured
# no sub request is processed and an error is returned to
# the user
batchEntitlement.maxRequests.includeOffers=10000
batchEntitlement.maxRequests.excludeOffers=65000

# email configuration
# hour
batchEntitlement.email.enable=true
batchEntitlement.email.retries=48
batchEntitlement.email.retryInterval=1
batchEntitlement.email.smtpserver=smtp3.hp.com

# instance function configuration
# batchEntitlement.processRequests.enable=true
# batchEntitlement.extractRequests.enable=true
# batchEntitlement.deleteRequests.enable=true

# specify the size of sub request list in memory. The default value is 50.
batchEntitlement.subRequestList.maxSize=50

