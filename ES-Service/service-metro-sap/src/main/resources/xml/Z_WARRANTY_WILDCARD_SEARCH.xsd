<?xml version="1.0" encoding="UTF-8"?>
        <xsd:schema  xmlns:s0="urn:sap-com:document:sap:rfc:functions" xmlns:xsd="http://www.w3.org/2001/XMLSchema"  targetNamespace="urn:sap-com:document:sap:rfc:functions">
            <xsd:element name="Z_WARRANTY_WILDCARD_SEARCH">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="PRODUCTNUM" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:maxLength value="18"></xsd:maxLength>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="SALESORDERNUM" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:maxLength value="14"></xsd:maxLength>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="SERIALNUM" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:maxLength value="30"></xsd:maxLength>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="TOTALRECORDRETURN" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:maxLength value="3"></xsd:maxLength>
                                    <xsd:pattern value="\d*"></xsd:pattern>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Z_WARRANTY_WILDCARD_SEARCH.Response">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="ZPRODUCTLIST" type="s0:ZWTYWILDCARD"></xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:complexType name="ZPRODLIST">
                <xsd:sequence>
                    <xsd:element name="item" minOccurs="0" maxOccurs="unbounded" type="s0:ZWTY_PRODLIST"></xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZWTYERROR">
                <xsd:sequence>
                    <xsd:element name="ID" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="100"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZWTYERRORTAB">
                <xsd:sequence>
                    <xsd:element name="item" minOccurs="0" maxOccurs="unbounded" type="s0:ZWTYERROR"></xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZWTYWILDCARD">
                <xsd:sequence>
                    <xsd:element name="PRODLIST" minOccurs="0" type="s0:ZPRODLIST"></xsd:element>
                    <xsd:element name="WTYMESSAGE" minOccurs="0" type="s0:ZWTYERRORTAB"></xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZWTY_PRODLIST">
                <xsd:sequence>
                    <xsd:element name="PRODUCTID" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="18"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="SERIALNUM" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="30"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="OOSKEY" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="18"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="PRODUCTDESC" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="40"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="SHIPTOCTRYCODE" minOccurs="0">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="2"></xsd:maxLength>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:schema>
