package com.hp.es.service.contractEntitlement.orchestration;

import java.util.HashMap;

import com.hp.es.service.contractEntitlement.EsReplyContext;
import com.hp.es.xml.service.EsReply;

/**
 * @author juhank
 */
public class ContractBySnReply {
	
	private EsReply _esReply = null;
	private HashMap _mappedProductList = null;
	private EsReplyContext _ctx = null;
	
	public ContractBySnReply(EsReply esReply, HashMap mappedProductList,EsReplyContext ctx) {
		_esReply = esReply;
		_mappedProductList = mappedProductList;
		_ctx= ctx;
	}
	
	public EsReply getEsReply() {
		return _esReply;
	}
	public HashMap getMappedProductList() {
		return _mappedProductList;
	}
	
	public EsReplyContext getEsReplyContext() {
		return _ctx;
	}
	
	
}
