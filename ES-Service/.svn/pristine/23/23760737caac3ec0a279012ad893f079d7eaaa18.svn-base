package com.hp.es.service.manufacturingHeaderInformation.orchestration;
/*
 * A temporary holder for the SNR exception
 */
public class ManufacturingInstalledBaseServiceError {
	private String errorId;
	private String errorDescription;
	/**
	 * 
	 */
	public ManufacturingInstalledBaseServiceError() {
		super();
	}
	/**
	 * @param errorId
	 * @param errorDescription
	 */
	public ManufacturingInstalledBaseServiceError(String errorId,
			String errorDescription) {
		super();
		this.errorId = errorId;
		this.errorDescription = errorDescription;
	}
	/**
	 * @return the errorId
	 */
	public String getErrorId() {
		return errorId;
	}
	/**
	 * @param errorId the errorId to set
	 */
	public void setErrorId(String errorId) {
		this.errorId = errorId;
	}
	/**
	 * @return the errorDescription
	 */
	public String getErrorDescription() {
		return errorDescription;
	}
	/**
	 * @param errorDescription the errorDescription to set
	 */
	public void setErrorDescription(String errorDescription) {
		this.errorDescription = errorDescription;
	}


	
	
	
	
	
}
